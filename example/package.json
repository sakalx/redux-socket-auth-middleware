{
  "name": "example",
  "version": "1.0.0",
  "description": "Example socket.io authentication",
  "main": "index.js",
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "browsers": [
              "> 1%",
              "ie >= 11",
              "not op_mini all"
            ]
          }
        }
      ],
      "@babel/react"
    ],
    "plugins": [
      "@babel/transform-runtime"
    ]
  },
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack --mode=production"
  },
  "keywords": [
    "auth",
    "authentication",
    "authorization",
    "middleware",
    "react",
    "redux",
    "redux-middleware",
    "socket",
    "socket.io"
  ],
  "author": "Sakal Serhii <sakals@mail.ua>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "favicons-webpack-plugin": "0.0.9",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "webpack-dev-server": "^3.2.1"
  }
}
